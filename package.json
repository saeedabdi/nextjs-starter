{
    "name": "elegant",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "find:unused": "next-unused",
        "lint": "eslint --ignore-path .gitignore --ext .tsx  ./",
        "lint:fix": "npm run lint -- --fix"
    },
    "next-unused": {
        "alias": {
            "@lib/*": [
                "lib/*"
            ],
            "@assets/*": [
                "assets/*"
            ],
            "@config/*": [
                "config/*"
            ],
            "@components/*": [
                "components/*"
            ]
        },
        "debug": true,
        "include": [
            "components",
            "lib",
            "pages"
        ],
        "exclude": [],
        "entrypoints": [
            "pages"
        ]
    },
    "dependencies": {
        "next": "latest",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "next-react-svg": "1.1.3",
        "next-redux-wrapper": "^7.0.2",
        "react-redux": "^7.2.4",
        "redux": "^4.1.0",
        "redux-saga": "^1.1.3",
        "@reduxjs/toolkit": "^1.6.0"
    },
    "devDependencies": {
        "@types/node": "17.0.4",
        "@types/react": "17.0.38",
        "autoprefixer": "^10.4.0",
        "postcss": "^8.4.5",
        "husky": "^4.3.7",
        "eslint": "^7.16.0",
        "eslint-config-prettier": "^7.1.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-prettier": "^3.3.0",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "eslint-plugin-unused-imports": "^1.1.2",
        "next-unused": "^0.0.6",
        "redux-devtools-extension": "^2.13.9",
        "prettier": "^2.5.1",
        "prettier-plugin-tailwindcss": "^0.1.1",
        "tailwindcss": "^3.0.7",
        "typescript": "4.5.4"
    }
}
